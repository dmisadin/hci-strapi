{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Products"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "ProductID": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "UID": {
      "type": "uid",
      "targetField": "ProductID",
      "required": true
    },
    "Name": {
      "type": "string",
      "required": true
    },
    "UnitsAvailable": {
      "type": "integer",
      "min": 0
    },
    "Color": {
      "type": "string"
    },
    "Height": {
      "type": "integer",
      "min": 0
    },
    "Width": {
      "type": "integer",
      "min": 0
    },
    "Length": {
      "type": "integer",
      "min": 0
    },
    "Voltage": {
      "type": "decimal",
      "min": 0
    },
    "WarrantyMonths": {
      "type": "integer",
      "min": 1,
      "default": 24,
      "required": true
    },
    "Mass": {
      "type": "decimal",
      "min": 0
    },
    "IngressProtection": {
      "type": "integer"
    }
  }
}
